  <body>

    <body>
    <div class="wrapper">
        <!-- PRICING-TABLE CONTAINER -->
        <div class="pricing-table group">
            <!-- PERSONAL -->
            <div class="block personal fl">
                <h2 class="title">Introduction</h2>
                <!-- CONTENT -->
                <div class="content">
                    <p class="price">
                        <sup>$</sup>
                        <span>149</span>
                        <sub>/mo.</sub>
                    </p>
                    <p class="hint">2 hours personal instruction monthly</p>
                </div>
                <!-- /CONTENT -->
                <!-- FEATURES -->
                <ul class="features">
                    <li class="strong">A savings of $90</li>
                    <li>Designed for young minds</li>
                    <li>Access to group sessions</li>
                    <li>Email and chat support</li>
                </ul>
                <!-- /FEATURES -->
                <!-- PT-FOOTER -->
                <div class="pt-footer">
                  <form action="/buy" class="two-hours-monthly-student" method="POST">
                    <input type="hidden" name="plan" value="two-hours-monthly-student" />
                    <script src="https://checkout.stripe.com/checkout.js"
                        class="stripe-button"
                        data-key="<%= STRIPE_PUB_KEY %>"
                        data-amount="14900"
                        data-name="RUBYMENTOR"
                        data-description="Introduction plan (149/mo)"
                        data-image="/images/rm-logo-128.png">
                    </script>
                  </form>
                </div>
                <!-- /PT-FOOTER -->
            </div>
            <!-- /PERSONAL -->

            <!-- PROFESSIONAL -->
            <div class="block professional fl">
                <h2 class="title">Accelerated</h2>
                <!-- CONTENT -->
                <div class="content">
                    <p class="price">
                        <sup>$</sup>
                        <span>299</span>
                        <sub>/mo.</sub>
                    </p>
                    <p class="hint">4 hours personal instruction monthly</p>
                </div>
                <!-- /CONTENT -->
                <!-- FEATURES -->
                <ul class="features">
                    <li class="strong">A savings of $180</li>
                    <li>Designed for young minds</li>
                    <li>Access to group sessions</li>
                    <li>Email and chat support</li>
                </ul>
                <!-- /FEATURES -->
                <!-- PT-FOOTER -->
                <div class="pt-footer">
                  <form action="/buy" class="four-hours-monthly-student" method="POST">
                    <input type="hidden" name="plan" value="four-hours-monthly-student" />
                    <script src="https://checkout.stripe.com/checkout.js"
                        class="stripe-button"
                        data-key="<%= STRIPE_PUB_KEY %>"
                        data-amount="29900"
                        data-name="RUBYMENTOR"
                        data-description="Accelerated plan (299/mo)"
                        data-image="/images/rm-logo-128.png">
                    </script>
                  </form>
                </div>
                <!-- /PT-FOOTER -->
            </div>
            <!-- /PROFESSIONAL -->

            <!-- BUSINESS -->
            <div class="block business fl">
                <h2 class="title">Apprenticeship</h2>
                <!-- CONTENT -->
                <div class="content">
                    <p class="price">
                        <sup>$</sup>
                        <span>599</span>
                        <sub>/mo.</sub>
                    </p>
                    <p class="hint">8 hours personal instruction monthly</p>
                </div>
                <!-- /CONTENT -->

                <!-- FEATURES -->
                <ul class="features">
                    <li class="strong">A savings of $360</li>
                    <li>Designed for young programmers</li>
                    <li>Access to group sessions</li>
                    <li>Email and chat support</li>
                </ul>
                <!-- /FEATURES -->

                <!-- PT-FOOTER -->
                <div class="pt-footer">
                  <form action="/buy" class="eight-hours-monthly-student" method="POST">
                    <input type="hidden" name="plan" value="eight-hours-monthly-student" />
                    <script src="https://checkout.stripe.com/checkout.js"
                        class="stripe-button"
                        data-key="<%= STRIPE_PUB_KEY %>"
                        data-amount="59900"
                        data-name="RUBY MENTOR"
                        data-description="Apprenticeship plan (599/mo)"
                        data-image="/images/rm-logo-128.png">
                    </script>
                  </form>
                </div>
                <!-- /PT-FOOTER -->
            </div>
            <!-- /BUSINESS -->

        </div>
        <!-- /PRICING-TABLE -->
    </div>
</body>
    
    
    
    
