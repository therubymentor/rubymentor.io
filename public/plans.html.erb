    <div class="wrapper container">
      <!-- PRICING-TABLE CONTAINER -->
      <div class="pricing-table row">
        <!-- PERSONAL -->
        <div class="block personal col-md-3 col-sm-1 nopad">
          <h2 class="title">Introduction</h2>
          <!-- CONTENT -->
          <div class="content">
            <p class="hint"><span>2 hours/mo.</span>1 on 1 Mentorship</p>
            <hr>
            <p class="price">
            <sup>$</sup>
            <span>199</span>
